/**app.wxss**/
page {
  box-sizing: border-box;
  background-color: #F8F8F8;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.clear-both{
  clear: both;
}
.color00ADFA{
  color: #00ADFA;
}
.color999999{
  color: #999999;
}
.fw500{
  font-weight: 500;
}
.fw550{
  font-weight: 550;
}
.fw600{
  font-weight: 600;
}
.fw700{
  font-weight: 700;
}
.flex1{
  flex: 1;
}
.tac{
  text-align: center;
}
.left-border{
  border-right:1px solid #E5E5E5;
}
.fz56{
  font-size: 56rpx;
}
.border-color-FFC002 {
    border-bottom: 2px solid #FFC002;
}
/* 分割线 */
.trans-line{
  background: #ddd;
  display: inline-block;
  width: 112rpx;
  height: 2rpx;
  vertical-align: middle;
}
.center{
  text-align: center;
}