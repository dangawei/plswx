.fs-cell-top-img{
  margin-bottom: 30rpx;
  text-align: center;
}
.fs-cell-top-img image{
  width: 124rpx;
  height: 124rpx;
}
.fs-cell-bottom-box{
  padding: 30rpx 30rpx 30rpx 30rpx;
  width: 630rpx;
  box-sizing: border-box;
  text-align: center;
  margin: 0 auto;
}